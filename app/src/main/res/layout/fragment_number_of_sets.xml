<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    style="@style/Fragment_player_select_Background">

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginLeft="10dp"
        android:layout_marginStart="10dp"
        android:text="@string/fragment_number_of_sets_TEXT"
        android:id="@+id/fragment_number_of_sets_TEXT"
        style="@style/Fragment_player_select_TextView">
    </TextView>

    <Spinner
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="@id/fragment_number_of_sets_TEXT"
        android:layout_marginRight="10dp"
        android:layout_marginEnd="10dp"
        android:id="@+id/fragment_number_of_sets_SPINNER" />


</androidx.constraintlayout.widget.ConstraintLayout>